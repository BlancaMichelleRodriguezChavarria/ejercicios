% Teorema del valor intermedio  F(x)= 3*x-2 [-2,3]   k=1

syms x;
fx= input("INGRESE LA FUNCION :")
funcion = inline (char(fx))
a= input("INGRESE EL VALOR DEL LIMITE INFERIOR a: "); 
b= input("INGRESE EL VALOR DEL LIMITE SUPERIOR b: ");
k= input ("INFRESE LA CONSTANTE K : ");
resultado1= 3*b-2 
resultado2= 3*a-2
total= resultado1< k < resultado1 


fprintf( "Es verdad que 1 esta entre los intervalos de -2 a 3");

x = -2:0.2:3;
f=x;
plot(f,x);
grid on
hold on
plot(f,x )
bar(f,x,"r")
title ("REPRESENTACION");  
